<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>airspeed.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>visses</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <object-name>tour</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <interpolation>
     <entry><ind>  0</ind><dep>    0</dep></entry>
     <entry><ind> 27</ind><dep>   11</dep></entry>
     <entry><ind> 54</ind><dep>  65</dep></entry>
     <entry><ind> 81</ind><dep>  124</dep></entry>
     <entry><ind> 108</ind><dep>  180</dep></entry>
     <entry><ind> 135</ind><dep>  240</dep></entry>
     <entry><ind> 162</ind><dep>  293</dep></entry>
     <entry><ind> 189</ind><dep>  345</dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>


